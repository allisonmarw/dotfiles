{
  "*": {
    "Dockerfile": {
      "dispatch:": "docker build",
      "start": "docker run -f {}",
      "alternate": "docker-compose.{}"
    },
    "docker-compose.*": {
      "make:": "docker-compose build",
      "start": "docker-compose up -d -f docker-compose.{}"
    },
    "plugin/*.vim": {
        "type": "plugin"
    },
    "ftdetect/*.vim": {
        "type": "plugin"
    },
    "autoload/*.vim": {
        "type": "autoload"
    },
    "docs/*.txt": {
        "type": "doc"
    },
    "README*"
        "type": "doc"
    }
  }
}
