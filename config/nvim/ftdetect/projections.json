{
  "/Users/aping1/Library/Application Support/UÌˆbersicht/widgets/*": {
    "*index.jsx" : {
      "type": "source",
      "skeleton": "ubersicht_widget",
      "alternate": "/U{}some_script.applescript"
    },
    "*some_script.applescript" : {
      "type": "source",
      "skeleton": "json_applescript",
      "alternate": "/U{}index.jsx"
    }
  },
  "*": {
    "Dockerfile*": {
      "make:": "docker build {}",
      "console": "docker run -f {} -- /bin/bash",
      "alternate": "docker-compose.{}"
    },
    "docker-compose.*": {
      "make:": "docker-compose build",
      "start": "docker-compose up -d -f docker-compose.{}"
    },
    "plugin/*.vim": {
      "type": "plugin"
    },
    "ftdetect/*.vim": {
      "type": "plugin"
    },
    "autoload/*.vim": {
      "type": "autoload"
    },
    "README.md": {
      "type": "readme",
      "skeleton" : "readme"
    },
    "*.applescript": {
      "make": "osacompile -o {}.scpt {}.applescript",
      "start":"osascript {}.applescript"
    },
    "test_*.py": {
      "type": "test",
      "alternate": "{}.py",
      "skeleton": "click_unittest"
    },
    "click_*.py": {
      "type": "source",
      "alternate": "test_{}.py",
      "skeleton": "click_command"
    },
    "setup.py": {
      "type": "setuptools",
      "make": "python3 setup.py sdist bdist_wheel",
      "skeleton": "python_setup"
    }
  }
}
